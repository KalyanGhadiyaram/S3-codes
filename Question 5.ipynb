   "source": [
    "from numpy.random import rand\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "from scipy.stats import expon\n",
    "from scipy.stats import poisson\n",
    "import matplotlib.pyplot as plt\n",
    "import statistics as st\n",
    "import pandas as pd"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "4e4c1d7d",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Obtaining a range of mRNA production rates over a wide range of magnitudes (figure 11)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "f8fa1ca0",
   "metadata": {},
   "outputs": [],
   "source": [
    "# To generate a random set of numbers with a wide range of magnitudes we make a set called random_set and multiply the value 0.2\n",
    "# by a number of 10 which is multiplied 500 times by a power in a set of uniform numbers created by np.uniform with \n",
    "# a range of -3 to 3.\n",
    "random_set = 0.2* np.power(10, np.random.uniform(-3, 3, 500))\n",
    "\n",
    "# Create a histogram to visualize the distribution\n",
    "plt.figure(figsize=(8, 6))\n",
    "\n",
    "# Plot the histogram with logarithmic bins and a logarithmic x-axis scale\n",
    "plt.hist(random_set, bins=np.logspace(-4, 4, 50), color='c', edgecolor='k', alpha=0.65)\n",
    "plt.xscale('log')\n",
    "\n",
    "# Add labels and title\n",
    "plt.xlabel('Range of magnitudes')\n",
    "plt.ylabel('Frequency')\n",
    "plt.title('Range of transcription magnitudes')\n",
    "plt.grid(True, which=\"both\", linestyle=\"--\", alpha=0.5)\n",
    "\n",
    "plt.show()\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "da3ce42e",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Obtaining values for mRNA expression level, protein expression level, variances and coefficients of variations. "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "4a5ce3ff",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Set the values of the rates\n",
    "k1 = 0.01\n",
    "k2 = 5\n",
    "k3 = 1\n",
    "\n",
    "# Set the stoichiometries\n",
    "stoichiometry = [1, -1]\n",
    "\n",
    "# Make a function to calculate the propensities of synthesis (k0) and degradation (k1). Because k0 varies from model to model, \n",
    "# we need to add this as an extra parameter in our function.\n",
    "def propensities_x(x, k0):\n",
    "    return [k0, k1 * x]\n",
    "\n",
    "# Define the propensities of the protein (defined as y). Because production of y is dependent on x, both values are necessary\n",
    "def propensities_y(x, y):\n",
    "    return [k2 * x, k3 * y]\n",
    "\n",
    "# Now define the function for the reaction times of x. This function describes how the value of x changes over time. This time\n",
    "# we need to include k0 into our propensities function as k0 is one of the propensities_x's input values.\n",
    "def reaction_times_x(x):\n",
    "    a = propensities_x(x, k0)\n",
    "    aInv = [1 / s if s > 0 else np.inf for s in a]\n",
    "    return -np.log(rand(2)) * aInv\n",
    "\n",
    "# Now define the function for the reaction times for y.\n",
    "# Once more, because production of y is dependent on x, both are necessary for this function.\n",
    "def reaction_times_y(x, y):\n",
    "    a = propensities_y(x, y)\n",
    "    aInv = [1 / s if s > 0 else np.inf for s in a]\n",
    "    return -np.log(rand(2)) * aInv\n",
    "\n",
# We now define the function for the ssa_step for mRNA (x). To avoid a computationally taxing loop, we will set a third parameter which is maximum
#iteration (max_iter). We will run the reaction using a while  function. 
    "def ssa_step_x(x, tIn, tOut, max_iter=500):\n",
    "    t = tIn\n",
    "    iter_count = 0\n",
    "    while t < tOut and iter_count < max_iter:\n",
    "        rt = reaction_times_x(x)\n",
    "        idx = np.argmin(rt)\n",
    "        tau = np.min(rt)\n",
    "        x += stoichiometry[idx]\n",
    "        t += tau\n",
    "        iter_count += 1\n",
    "    return x\n",
    "\n",
# We now define the function for the ssa_step for Protein (y).Just like ssa_step_x, to avoid a computationally taxing loop, we will set a 
#third parameter which is maximum iteration (max_iter). We will run the reaction using a while function. 
    "    t = tIn\n",
    "    iter_count = 0\n",
    "    while t < tOut and iter_count < max_iter:\n",
    "        rt = reaction_times_y(x, y)\n",
    "        idx = np.argmin(rt)\n",
    "        tau = np.min(rt)\n",
    "        y += stoichiometry[idx]\n",
    "        t += tau\n",
    "        iter_count += 1\n",
    "    return y\n",
    "\n",
    "# Now let us create empty vectors for each of the variables we want to measure. \n",
    "mRNA_mean_values = []\n",
    "mRNA_variance_values = []\n",
    "mRNA_coefficient_of_variation_values = []\n",
    "mRNA_noise_values = []\n",
    "protein_mean_values = []\n",
    "protein_variance_values = []\n",
    "protein_coefficient_of_variation_values = []\n",
    "protein_noise_values = []\n",
    "k0_values = []\n",
    "\n",
    "\n",
    "for j in random_set:\n",
    "    k0 = j\n",
    "    x = k0/k1 # We set the mRNA start value as equalling k0/k1, which is the steady state as derived from the poisson distribution\n",
    "    y = 5*x # We set protein to equal five times the starting value of x as protein production is equal to 5 times the value of mRNA\n",
    "    t = 0.0 \n",
    "    dt = 1\n",
    "    mRNA = [x]\n",
    "    protein = [y]\n",
    "    # Update k0 for this simulation\n",
    "\n",
    "\n",
    "    for i in range(1000):\n",
    "        x = ssa_step_x(x, t, t + dt)\n",
    "        y = ssa_step_y(x, y, t, t + dt)  # Pass x to ssa_step_y\n",
    "        t += dt\n",
    "        mRNA.append(x)\n",
    "        protein.append(y)\n",
    "# We then append the lists with the calculated values\n",
    "    k0_values.append(k0)\n",
    "    mRNA_mean_values.append(np.mean(mRNA[1:]))  # Exclude the initial value\n",
    "    mRNA_variance_values.append(np.var(mRNA[1:]))\n",
    "    mRNA_coefficient_of_variation_values.append(np.std(mRNA[1:]) / np.mean(mRNA[1:]))\n",
    "    mRNA_noise_values.append(np.var(mRNA[1:]) / np.square(np.mean(mRNA[1:])))\n",
    "    protein_mean_values.append(np.mean(protein[1:]))  # Exclude the initial value\n",
    "    protein_variance_values.append(np.var(protein[1:]))\n",
    "    protein_coefficient_of_variation_values.append(np.std(protein[1:]) / np.mean(protein[1:]))\n",
    "    protein_noise_values.append(np.var(protein[1:]) / np.square(np.mean(protein[1:])))\n",
    "\n",
    "    "
   ]
